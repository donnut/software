<!DOCTYPE registry SYSTEM "theregistry.dtd">
<registry>

 <domains>

  <domain>tp-robot
   <ref>Home page<url>http://www.translationproject.org/
   <ref>Project site<url>https://github.com/arunpersaud/TP-no-svn
   <mailto>test1@localhost
   <disclaim>
   <url>ftp://example.org/

  <domain>test1
   <ref>Home page<url>http://example.org
   <ref>Project site<url>http://example.org/devel
   <mailto>test2@localhost
   <url>ftp://example.org/

  <domain>test2
   <ref>Home page<url>http://example.org
   <ref>Project site<url>http://example.org/devel2
   <mailto>test3@localhost
   <url>ftp://example.org/

 </domains>

 <teams>

  <team>Brazilian Portuguese<code>pt_BR
   <mailto>ldpbr-translation@example.org
   <charset>UTF-8
   <leader>Foo Bar

   <translator>Foo Bar
    <mailto>foobar@example.org
    <disclaimer>

   <translator>Bar Foo
    <mailto>foobar2@example.org

  <team>Finnish<code>fi
   <mailto>translation-team-fi@example.org
   <leader>Another Name
   <ref>Mailing list info<url>https://lists.example.org/lists/listinfo/translation-team-fi

   <translator>Another Name
    <alias>with an alias
    <mailto>email1@example.org
    <mailto>email2@example.org
    <disclaimer><autosend>
    <remark>wants to do a lot

 </teams>
</registry>

